{% extends 'base.html.twig' %}

{% block title %}Edit profile !{% endblock %}

{% block body %}
    <div class="container">

        {{ form_start(form) }}
        {{ form_widget(form) }}

        <button type="submit" class="btn btn-primary">
            <i class="fa fa-save" aria-hidden="true"></i> {{ 'action.save'|trans }}
        </button>
        {{ form_end(form) }}

    </div>
{% endblock %}