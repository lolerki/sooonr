{
    "name": "symfony/skeleton",
    "type": "project",
    "license": "MIT",
    "description": "A minimal Symfony project recommended to create bare bones applications",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/api-pack": "^1.2",
        "doctrine/annotations": "^1.6",
        "excelwebzone/recaptcha-bundle": "^1.5",
        "google/recaptcha": "^1.2",
        "knpuniversity/oauth2-client-bundle": "^1.27",
        "league/oauth2-client": "^2.4",
        "league/oauth2-facebook": "^2.0",
        "league/oauth2-google": "^3.0",
        "riskio/oauth2-auth0": "^2.0",
        "rollerworks/password-strength-bundle": "^2.0",
        "rollerworks/password-strength-validator": "^1.1",
        "ruflin/elastica": "^6.1",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "4.1.*",
        "symfony/console": "^4.0",
        "symfony/finder": "4.1.*",
        "symfony/flex": "^1.2",
        "symfony/form": "4.1.*",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.3",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "4.1.*",
        "symfony/serializer": "4.1.*",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "4.1.*",
        "symfony/twig-bundle": "4.1.*",
        "symfony/validator": "4.1.*",
        "symfony/webpack-encore-bundle": "^1.2",
        "symfony/yaml": "^4.0",
        "vich/uploader-bundle": "^1.9"
    },
    "require-dev": {
        "api-platform/schema-generator": "^2.1",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "fzaninotto/faker": "^1.8",
        "symfony/dotenv": "4.1.*",
        "symfony/maker-bundle": "^1.11",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "*",
            "require": "4.1.*"
        }
    }
}
